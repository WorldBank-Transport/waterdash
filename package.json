{
  "name": "tanzania-water-dashboard",
  "version": "0.0.0",
  "description": "water dashboard",
  "license": "GPL-3.0",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && webpack && npm run _copy-stuff",
    "clean": "rm -fr dist",
    "_copy-stuff": "cd app && cp -r index.html favicon.ico images ../dist/",
    "lint": "npm run lint-scripts && npm run lint-styles",
    "lint-scripts": "eslint 'app/**/*.{es6,js,jsx}'",
    "lint-styles": "gulp lint-styles",
    "start": "node server.js",
    "test": "npm run lint && npm run test-only",
    "test-only": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/results"
    ],
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "es6",
      "js",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WorldBank-Transport/waterdash.git"
  },
  "keywords": [
    "tanzania",
    "water",
    "dashboard"
  ],
  "bugs": {
    "url": "https://github.com/WorldBank-Transport/waterdash/issues"
  },
  "homepage": "https://github.com/WorldBank-Transport/waterdash#readme",
  "engines": {
    "node": ">=3.0.0"
  },
  "devDependencies": {
    "babel-core": "5.8.24",
    "babel-eslint": "4.1.3",
    "babel-jest": "5.3.0",
    "babel-loader": "5.3.2",
    "css-loader": "0.18.0",
    "eslint": "1.5.0",
    "eslint-plugin-react": "3.4.2",
    "extract-text-webpack-plugin": "0.8.2",
    "gulp": "3.9.0",
    "gulp-sass-lint": "1.0.1",
    "jest-cli": "0.5.7",
    "node-sass": "3.3.2",
    "react-hot-loader": "1.3.0",
    "sass-loader": "2.0.1",
    "style-loader": "0.12.4",
    "webpack": "1.12.1",
    "webpack-dev-server": "1.11.0"
  },
  "dependencies": {
    "classnames": "2.1.3",
    "es6-promise": "3.0.2",
    "leaflet": "0.7.5",
    "lodash": "3.10.1",
    "object-assign": "4.0.1",
    "react": "0.13.3",
    "react-d3": "0.4.0",
    "react-font-awesome": "https://github.com/janmyler/react-font-awesome/tarball/261c1719581e9125a544f8b5b9efc2e27073d970",
    "react-leaflet": "0.7.0",
    "react-router": "1.0.0-rc1",
    "reflux": "0.2.12",
    "results": "0.5.1",
    "whatwg-fetch": "0.9.0"
  }
}
